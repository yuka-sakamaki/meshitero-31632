<div class="contents row">
  <div class="container">
    <h3>編集する</h3>
    <%= form_with(model: @tweet, local: true) do |form| %>
      <%= form.text_field :image, placeholder: "Image" %>
      <%= form.text_field :text, placeholder: "text", rows: "10" %>
      <%= form.text_field :tag_ids,value: @tag_list,\
    placeholder: "タグをつける。複数つけるには','で区切ってください。" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>