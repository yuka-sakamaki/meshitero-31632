<div class="contents row">
  <div class="container">
    <h3>編集する</h3>
    <%= form_with(model: @tweet, local: true) do |form| %>
      <%= form.text_field :image, placeholder: "Image" %>
      <%= form.text_field :text, placeholder: "text", rows: "10" %>
      <%= form.text_area :name, placeholder: "tag" %>
      <%= form.submit "SEND" %>
    <% end %>
  </div>
</div>